<h1>Fetch</h1>

@section Scripts{
    <script>
        fetch("/Api/Index")
        .then(re=>{
            if(re.ok){
                    console.log(re)
                return re.text(); //promise
            }
        })
        .then(data=>{
            console.log(data)
        })
        .catch(err=>{

        })
    </script>
}