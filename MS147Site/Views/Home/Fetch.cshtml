<h1>Fetch</h1>
<img id="img"/>

@section Scripts{
    <script>
        fetch('@Url.Content("~/Api/GetImageByte")')
        .then(re=>{
            if(re.ok){
                return re.blob(); //promise
            }
        })
        .then(data=>{
            $("#img").attr("src",URL.createObjectURL(data));
        })
        .catch(err=>{

        })
    </script>
}